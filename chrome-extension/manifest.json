{
	"manifest_version": 2,

	"name": "Error Logger",
	"description": "This extension logs JavaScript errors in page.",
	"author": "Jeremy Herr",
	"version": "0.1",
	"permissions": ["tabs", "http://*/*"],
	"content_scripts": [
		{
			"matches": [
				"http://localhost/*",
				"http://www.jeremyherr.org/*"
			],
			"js": [
				"socket.io.js",
				"on_error_start.js"
			],
			"run_at": "document_start",
			"all_frames": true
		}
	]
}